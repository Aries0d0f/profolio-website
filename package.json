    "@types/color": "^3.0.3",
    "@types/dompurify": "^2.4.0",
    "@types/jsdom": "^20.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^9.11.1",
