    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-plugin-vue": "^9.9.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.6",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^4.9.3",
    "uuid": "^9.0.0",
